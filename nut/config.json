{
  "name": "Network UPS Tools",
  "version": "1.1",
  "slug": "jumping-nut",
  "description": "Manage Uninterruptible Power Supplies",
  "url": "https://github.com/jumping2000/hassio-addons/tree/master/nut"
  "arch": [
  "amd64",
  "armhf"
  ],
  "startup": "before",
  "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
  "ports": {
    "3493/tcp": 3493
  },
  "boot": "auto",
  "options": {
    "logins": [
      { "username": "username", "password": "password", "instcmds": "all" }
    ],
    "battery": [
      { "battery_voltage_high": 13.0, "battery_voltage_low": 10.4, "charge time": 43200 }
    ],
    "runtimecal": [
      { "runtime1": 460, "power1": 100, "runtime2": 880, "power2": 50 }
    ],
    "ups": [
      { "upsname": "tecnoware1100", "driver": "blazer_usb", "port": "auto", "vendorid": "0665", "productid": "5161", "desc": "Tecnoware ERA 1100" }
    ],
    "bindaddr": "0.0.0.0",
    "bindport": "3493",
    "mode": "standalone"
  },
  "schema": {
    "logins": [
      { "username": "str", "password": "str", "instcmds": "str" }
    ],
    "battery": [
      { "battery voltage high": "float?", "battery voltage low": "float?", "charge time": "int?" }
    ],
    "runtimecal": [
      { "runtime1": "int?", "power1": "int?", "runtime2": "int?", "power2": "int?" }
    ],
    "ups": [
      { "upsname": "str", "driver": "str", "port": "str", "vendorid": "str?", "productid": "str?", "desc": "str" }
    ],
    "bindaddr": "str",
    "bindport": "port",
    "mode": "str"
  }
}
