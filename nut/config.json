{
  "name": "Network UPS Tools",
  "version": "0.1",
  "slug": "nut",
  "description": "Manage Uninterruptible Power Supplies",
  "arch": [
  "amd64",
  "armhf"
  ],
  "startup": "before",
  "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
  "ports": {
    "3493/tcp": 3493
  },
  "boot": "auto",
  "options": {
    "logins": [
      { "username": "nut", "password": "nut", "instcmds": "all" }
    ],
    pollinterval": "2",
    "ups": [
      { "upsname": "tecnoware", "driver": "blazer_usb", "port": "auto", "vendorid": "0665", "productid": "5161", "desc": "Tecnoware ERA" }
    ],
    "bindaddr": "0.0.0.0",
    "bindport": "3493",
    "mode": "standalone"
  },
  "schema": {
    "logins": [
      { "username": "str", "password": "str", "instcmds": "str" }
    ],
    "pollinterval": "int",
    "ups": [
      { "upsname": "str", "driver": "str", "port": "str", "vendorid": "str", "productid": "str", "desc": "str"}
    ],
    "bindaddr": "str",
    "bindport": "port",
    "mode": "str"
  }
}
